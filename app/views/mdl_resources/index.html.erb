<p id="notice"><%= notice %></p>

<h1>Mdl Resources</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Name</th>
      <th>Intro</th>
      <th>Introformat</th>
      <th>Tobemigrated</th>
      <th>Legacyfiles</th>
      <th>Legacyfileslast</th>
      <th>Display</th>
      <th>Displayoptions</th>
      <th>Filterfiles</th>
      <th>Revision</th>
      <th>Timemodified</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mdl_resources.each do |mdl_resource| %>
      <tr>
        <td><%= mdl_resource.course %></td>
        <td><%= mdl_resource.name %></td>
        <td><%= mdl_resource.intro %></td>
        <td><%= mdl_resource.introformat %></td>
        <td><%= mdl_resource.tobemigrated %></td>
        <td><%= mdl_resource.legacyfiles %></td>
        <td><%= mdl_resource.legacyfileslast %></td>
        <td><%= mdl_resource.display %></td>
        <td><%= mdl_resource.displayoptions %></td>
        <td><%= mdl_resource.filterfiles %></td>
        <td><%= mdl_resource.revision %></td>
        <td><%= mdl_resource.timemodified %></td>
        <td><%= link_to 'Show', mdl_resource %></td>
        <td><%= link_to 'Edit', edit_mdl_resource_path(mdl_resource) %></td>
        <td><%= link_to 'Destroy', mdl_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mdl Resource', new_mdl_resource_path %>
