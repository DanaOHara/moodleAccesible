<p id="notice"><%= notice %></p>

<h1>Mdl Role Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Roleid</th>
      <th>Contextid</th>
      <th>Userid</th>
      <th>Timemodified</th>
      <th>Modifierid</th>
      <th>Component</th>
      <th>Itemid</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mdl_role_assignments.each do |mdl_role_assignment| %>
      <tr>
        <td><%= mdl_role_assignment.roleid %></td>
        <td><%= mdl_role_assignment.contextid %></td>
        <td><%= mdl_role_assignment.userid %></td>
        <td><%= mdl_role_assignment.timemodified %></td>
        <td><%= mdl_role_assignment.modifierid %></td>
        <td><%= mdl_role_assignment.component %></td>
        <td><%= mdl_role_assignment.itemid %></td>
        <td><%= mdl_role_assignment.sortorder %></td>
        <td><%= link_to 'Show', mdl_role_assignment %></td>
        <td><%= link_to 'Edit', edit_mdl_role_assignment_path(mdl_role_assignment) %></td>
        <td><%= link_to 'Destroy', mdl_role_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mdl Role Assignment', new_mdl_role_assignment_path %>
