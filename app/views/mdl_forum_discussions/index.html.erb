<p id="notice"><%= notice %></p>

<h1>Mdl Forum Discussions</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Forum</th>
      <th>Name</th>
      <th>Firstpost</th>
      <th>Userid</th>
      <th>Groupid</th>
      <th>Assessed</th>
      <th>Timemodified</th>
      <th>Usermodified</th>
      <th>Timestart</th>
      <th>Timeend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mdl_forum_discussions.each do |mdl_forum_discussion| %>
      <tr>
        <td><%= mdl_forum_discussion.course %></td>
        <td><%= mdl_forum_discussion.forum %></td>
        <td><%= mdl_forum_discussion.name %></td>
        <td><%= mdl_forum_discussion.firstpost %></td>
        <td><%= mdl_forum_discussion.userid %></td>
        <td><%= mdl_forum_discussion.groupid %></td>
        <td><%= mdl_forum_discussion.assessed %></td>
        <td><%= mdl_forum_discussion.timemodified %></td>
        <td><%= mdl_forum_discussion.usermodified %></td>
        <td><%= mdl_forum_discussion.timestart %></td>
        <td><%= mdl_forum_discussion.timeend %></td>
        <td><%= link_to 'Show', mdl_forum_discussion %></td>
        <td><%= link_to 'Edit', edit_mdl_forum_discussion_path(mdl_forum_discussion) %></td>
        <td><%= link_to 'Destroy', mdl_forum_discussion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mdl Forum Discussion', new_mdl_forum_discussion_path %>
